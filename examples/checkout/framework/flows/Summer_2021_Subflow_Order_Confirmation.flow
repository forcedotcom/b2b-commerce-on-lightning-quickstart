    <decisions>
        <description>Checks that all conditions are met to close the cart</description>
        <name>Should_Close_Cart</name>
        <label>Should Close Cart</label>
        <locationX>398</locationX>
        <locationY>252</locationY>
        <defaultConnector>
            <targetReference>Get_Order_Summary_ID</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Do_Close_Cart</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>shouldArchiveCheckoutAndCloseCart</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>Get_Newest_Cart.Status</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Checkout</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Checkout_Session_State</targetReference>
            </connector>
            <doesRequireRecordChangedToMeetCriteria>false</doesRequireRecordChangedToMeetCriteria>
            <label>Do Close Cart</label>
        </rules>
    </decisions>
