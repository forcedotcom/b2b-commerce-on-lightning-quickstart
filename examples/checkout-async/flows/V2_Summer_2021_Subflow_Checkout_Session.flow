            <name>valid_no_expected_state</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>expCurrentState</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
