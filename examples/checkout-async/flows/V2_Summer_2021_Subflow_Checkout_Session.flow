        </rules>
        <rules>
            <name>valid_expected_match_state</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>expCurrentState</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
